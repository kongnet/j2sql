{
  "name": "j2sql",
  "version": "0.0.1",
  "description": "j2sql自用函数",
  "main": "index.js",
  "scripts": {
    "test": "mocha -r co-mocha --reporter spec --timeout 100000 --recursive tests/",
    "coveralls": "istanbul cover _mocha -- -r co-mocha --reporter spec --timeout 100000 --recursive tests/ && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kongnet/j2sql.git"
  },
  "author": "Kongnet <9601698@qq.com>",
  "license": "MIT",
  "dependencies": {
    "meeko": "*",
    "mysql": "*",
    "hprose": "*",
    "co-mysql": "*"
  },
  "devDependencies": {
    "mocha": "*",
    "co-mocha": "*",
    "istanbul": "*",
    "coveralls": "*"
  },
  "eslintConfig": {
    "plugins": [
      "html"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": true
    },
    "globals": {
      "angular": true
    },
    "rules": {
      "no-unused-vars": 1,
      "camelcase": 0,
      "curly": 2,
      "brace-style": [
        2,
        "1tbs"
      ],
      "quotes": [
        1,
        "single"
      ],
      "semi": [
        1,
        "always"
      ],
      "space-infix-ops": 1,
      "no-console": 1,
      "complexity": [
        1,
        8
      ]
    },
    "extends": "eslint:recommended"
  }
}